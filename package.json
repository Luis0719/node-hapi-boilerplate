{
  "name": "hapi-node-jwt",
  "version": "1.0.0",
  "description": "Template for Hapi APIs",
  "main": "src/index.js",
  "scripts": {
    "start:dev": "node src/app.js",
    "start:prod": "node src/app.js",
    "watch": "nodemon src/app.js",
    "test": "APP_ENV=test mocha test/**/*.spec.js",
    "make-pretty": "prettier --write '{src,test}/**/*.js'",
    "migrate": "npx sequelize-cli db:migrate",
    "unmigrate": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "unseed": "npx sequelize-cli db:seed:undo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Luis0719/node-hapi-boilerplate.git"
  },
  "author": "Luis0719",
  "license": "ISC",
  "homepage": "https://bitbucket.org/luis0719/fox-adminjs#readme",
  "customDependencies": {
    "config": "./config",
    "common": "./src/common"
  },
  "dependencies": {
    "@hapi/basic": "^6.0.0",
    "@hapi/boom": "^9.1.0",
    "@hapi/hapi": "^19.1.1",
    "@hapi/inert": "^6.0.1",
    "@hapi/vision": "^6.0.0",
    "bcrypt": "^5.0.0",
    "bluebird": "^3.7.2",
    "hapi-auth-bearer-token": "^6.1.6",
    "hapi-jsonwebtoken": "^1.1.0",
    "hapi-pino": "^8.3.0",
    "hapi-swagger": "^13.0.2",
    "jiggler": "^1.0.1",
    "joi": "^14.3.1",
    "lodash": "^4.17.15",
    "luxon": "^1.25.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "ramda": "^0.27.0",
    "require-directory": "^2.1.1",
    "sequelize": "^6.3.3"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-mocha": "^8.0.0",
    "mocha": "^8.1.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.1.1",
    "sequelize-cli": "^6.2.0"
  },
  "build": "localdev"
}
